{
  "name": "pokedex",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "type": "module",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "run-s lint:typescript lint:code",
